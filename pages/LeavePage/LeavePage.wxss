page {
    background-color: #f0f4f5;
}

/*页面  */

.page_one {
    position: absolute;
    top: 50rpx;
    left: 0;
    display: flex;
    font-size: 28rpx;
}

/*内容内页面上  */

.page_up {
    width: 710rpx;
    background-color: #fff;
    box-shadow: 5rpx 5rpx 5rpx #dfe3e6;
    border-radius: 10rpx;
    margin: 0 0 0 20rpx;
}

.My_title {
    background: #e8f2ed;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-weight: 600;
    border-radius: 10rpx 10rpx 0 0;
}

/*请假选择角标*/

.LeavePic {
    width: 20rpx;
    height: 20rpx;
}

/*所有高度相同的view  */

.AllSameHeight {
    height: 100rpx;
    background-color: #fff;
    border-bottom: 1px solid #eeeded;
    line-height: 100rpx;
}

/*添加图片外面的view  */

.Pictures {
    position: absolute;
    min-height: 140rpx;
}

.LeaveHeight {
    min-height: 300rpx;
    background-color: #fff;
    line-height: 100rpx;
    margin-bottom: 20rpx;
      border-bottom: 1px solid #eeeded;
}

/*请假类型字体颜色  */

.LeaveType {
    color: #30a072;
}

/*左边竖排字对其  */

.Title_left {
    width: 180rpx;
    margin-left: 20rpx;
}

.Title_left text {
    padding-left: 10px;
}

/*右边竖排字对其  */

.Title_right {
    width: 470rpx;
    margin-left: 20rpx;
    text-align: right;
}

/*日期时间  */

.Title_rightFrame {
    width: 300rpx;
    margin-left: 210rpx;
}

.Title_rightFrame view {
    padding-left: 20rpx;
}

.Pictures image {
    margin: 10px;
}

/*请假原因  */

.Title_righdtFrame .text {
    white-space: nowrap;
    height: 300rpx;
    line-height: 40rpx;
    margin: 10rpx auto;
    width: 90%;
    background: #f0f4f5;
}

/*申请时长   添加删除  */

.Title_right button {
    width: 60rpx;
    height: 60rpx;
}

.Title_right  .AddNum {
    width: 100rpx;
    height: 100rpx;
    line-height: 60rpx;
    text-align: center;
}

.hold_button {
    width: 180rpx;
    height: 60rpx;
    margin-top: 20rpx;
    margin-left: 300rpx;
}

.hold_button  view {
    margin-right: 10rpx;
}

/*加减号  */

.hold_button .add_button {
    width: 60rpx;
    height: 60rpx;
}

.AddPicture {
    width: 30rpx;
    height: 30rpx;
}

.hold_button .add_button .AddPicture {
    margin-bottom: 20rpx;
}

.hold_button .reduce .AddPicture {
    margin-bottom: 20rpx;
}

/*按钮  */

.page_down {
    display: flex;
    width: 710rpx;
    margin: 0 0 24rpx 20rpx;
}

.page_down view {
    width: 50%;
}

.page_down .SaveAndLay_button {
    width: 200rpx;
    height: 60rpx;
    line-height: 60rpx;
    background-color: #3edd9b;
    margin-top: 30rpx;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
}

.SaveAndLay_button::after {
    border: none;
}

/*已经上传的图片样式  */

.Pictures {
    position: relative;
    min-height: 140rpx;
}

/*上传的图片  */

.tempImg {
    width: 300rpx;
    height: 300rpx;
}

.Pictures .UploadPic {
    min-height: 100rpx;
    min-width: 500rpx;
    margin-bottom: 100rpx;
    margin-left: 20rpx;
}

.Pictures .Pic_add {
    position: absolute;
    bottom: 10rpx;
    padding-left: 500rpx;
}

/*上传图片角标  */

.AddPic {
    width: 60rpx;
    height: 60rpx;
    margin: 10px;
}

/*背景圖片  */

.container_Bg {
    width: 100%;
    height: 400rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: cover;
    z-index: -1;
}
