/*输入框  */

.cardNumber {
    width: 400rpx;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 28rpx;
    margin: 100rpx auto;
    background: #5feab0;
    border-radius: 10rpx;
    text-align: center;
    color: #fff;
}

.phcolor {
    color: #fff;
}

/*立即登录  */

.goIndex {
    width: 400rpx;
    height: 70rpx;
    line-height: 70rpx;
    background: #5feab0;
    border-radius: 10rpx;
    font-size: 28rpx;
    color: #fff;
}

.goIndex::after{
    border: none;
}

/* logo */

.pic {
    width: 100rpx;
    height: 100rpx;
    animation: goRoute 2s infinite;
}

/*腾放OA  */

.font {
    color: #30a073;
}

.logo {
    width: 100rpx;
    height: 100rpx;
    margin: 80rpx auto;
}

.flexCol {
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
}

/*登录背景 */

.loginBg {
    width: 100%;
    height: 402rpx;
    position: fixed;
    bottom: 0;
    z-index: 1;
}
